<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_pressed="true" >
        <shape>
            <solid
                android:color="#1298d6" />
            <stroke
                android:width="1dp"
                android:color="#3986c9" />
            <corners
                android:radius="2dp" />
        </shape>
    </item>
    <item>
        <shape>
            <stroke
                android:width="1dp"
                android:color="#3189c9" />
            <corners
                android:radius="2dp" />

        </shape>
    </item>
</selector>